pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
               sh 'cd coit-backend1 && mvn install'
            }
        }
        stage('docker-build') {
            steps {
               sh 'cd coit-backend1 && docker build -t danushvithiyarth/coit-backend1:v2 .'
               sh 'docker images' 
            }
        }
        stage('docker-push') {
           steps {
              withCredentials([usernamePassword(credentialsId: '02fb8a91-02e0-4db2-9cec-7f24aef61258', usernameVariable: 'danushvithiyarth', passwordVariable: 'fodjo1-qatgoP-fixsyq')]) {
                    sh 'echo $DOCKERHUB_PASS | docker login -u $DOCKERHUB_USER --password-stdin'
                    sh 'docker push danushvithiyarth/coit-backend1:v2'
           }
    }
}
}
}
