@Library(sharedlib)
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                first()
                sh 'echo "build"'
               //sh 'cd coit-backend1 && mvn install'
            }
        }
        stage('docker-build') {
            steps {
                sh 'echo "docker build"'
              // sh 'cd coit-backend1 && docker build -t danushvithiyarth/coit-backend1:v2 .'
              // sh 'docker images' 
            }
        }
        stage('docker-push') {
           steps {
               sh 'echo "docker push"'
                   // sh 'docker push danushvithiyarth/coit-backend1:v2'
           }
    }
}
}

